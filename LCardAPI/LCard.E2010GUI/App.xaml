<Application x:Class="LCard.E2010GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LCard.E2010GUI"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:startup="clr-namespace:LCard.E2010GUI.Startup"
             >
    <Application.Resources>
        <ResourceDictionary>
            <system:String x:Key="MainTitle">Управление модулем L-CARD E20-10</system:String>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <startup:AppBootstrapper x:Key="Bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
